{
  "name": "docurry",
  "version": "1.0.0",
  "description": "A spicy, zero-config static documentation site generator.",
  "main": "index.js",
  "scripts": {
    "dev": "clear && rm -rf ./build && NODE_ENV=development concurrently --names \"build,webserver\" \"npm:build\" \"wait-on ./build/.build.log && npx live-server --no-browser ./build\"",
    "build": "node build.js"
  },
  "keywords": [
    "documentation",
    "ssg",
    "static",
    "static-site-generator"
  ],
  "author": "Alexis Boni",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "chalk": "^4.1.0",
    "chokidar": "^3.4.2",
    "columnify": "^1.5.4",
    "concurrently": "^5.3.0",
    "directory-tree": "^2.2.4",
    "favicons": "^6.2.0",
    "flag-icon-css": "^3.5.0",
    "flat": "^5.0.2",
    "fs-extra": "^9.0.1",
    "fuse.js": "^6.4.1",
    "glob": "^7.1.6",
    "gray-matter": "^4.0.2",
    "highlight.js": "^10.2.0",
    "html-entities": "^1.3.1",
    "html-to-text": "^5.1.1",
    "jimp": "^0.16.1",
    "jsdom": "^16.4.0",
    "live-server": "1.2.1",
    "markdown-it": "^11.0.1",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-external-links": "0.0.6",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "minimatch": "^3.0.4",
    "mustache": "^4.0.1",
    "node-sass": "^4.14.1",
    "pretty": "^2.0.0",
    "purgecss": "^2.3.0",
    "spectre.css": "^0.5.9",
    "trianglify": "^4.0.0",
    "twemoji": "^13.0.1",
    "voca": "^1.4.0",
    "wait-on": "^5.2.0"
  }
}
